{
  "builds": [
    {
      "src": "api/tasa.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/tasas",
      "dest": "api/tasa.py"
    },
    {
      "src": "/api/tasa/(.*)",
      "dest": "api/tasa.py"
    },
    {
      "src": "/api/health",
      "dest": "api/tasa.py"
    }
  ]
}